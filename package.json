{
  "name": "vue-security-scanner",
  "version": "1.8.0",
  "description": "A comprehensive security scanning tool for Vue.js projects with rule-based vulnerability detection",
  "main": "src/scanner.js",
  "bin": {
    "vue-security-scanner": "bin/vue-security-scanner.js",
    "vsc": "bin/vue-security-scanner.js",
    "vue-security-distributed": "bin/vue-security-distributed.js",
    "vsd": "bin/vue-security-distributed.js"
  },
  "files": [
    "bin/",
    "src/",
    "config/",
    "examples/",
    "dashboard/",
    "LICENSE",
    "README.md",
    "README_CN.md",
    "RULE_EXTENSION_GUIDE.md",
    "QUICKSTART_CUSTOM_RULES.md",
    "CONFIGURATION.md",
    "USAGE_EXAMPLES.md",
    "VUE_SUPPORT.md",
    "DISTRIBUTED_SCANNING.md",
    "DASHBOARD.md",
    "vue-security-scanner.config.json"
  ],
  "scripts": {
    "start": "node --expose-gc bin/vue-security-scanner.js",
    "test": "jest",
    "scan": "node --expose-gc bin/vue-security-scanner.js",
    "scan-test": "node --expose-gc bin/vue-security-scanner.js tests/",
    "scan-with-gc": "node --expose-gc bin/vue-security-scanner.js",
    "distributed:scan": "node bin/vue-security-distributed.js scan",
    "distributed:worker": "node bin/vue-security-distributed.js worker",
    "dashboard": "node dashboard/server.js"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue3.6",
    "vapor-mode",
    "security",
    "scanner",
    "xss",
    "vulnerability",
    "audit",
    "sast",
    "dependency-check",
    "static-analysis",
    "vite",
    "rule-based",
    "vue2",
    "composition-api",
    "security-rules",
    "reactive-security",
    "vapor-security"
  ],
  "author": "ereddate",
  "license": "MIT",
  "homepage": "https://github.com/ereddate/vue-security-scanner#readme",
  "bugs": {
    "url": "https://github.com/ereddate/vue-security-scanner/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ereddate/vue-security-scanner.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "axios": "^1.6.2",
    "chalk": "^5.6.2",
    "cheerio": "^1.2.0",
    "commander": "^14.0.2",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "form-data": "^4.0.0",
    "glob": "^13.0.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "jest": "^30.2.0"
  }
}
